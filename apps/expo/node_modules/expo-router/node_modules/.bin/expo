#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/Code/Nodejs/Zola/node_modules/.pnpm/expo@50.0.21_@babel+core@7._dc68a35d979f9b90e1209db8fa280efd/node_modules/expo/bin/node_modules:/mnt/e/Code/Nodejs/Zola/node_modules/.pnpm/expo@50.0.21_@babel+core@7._dc68a35d979f9b90e1209db8fa280efd/node_modules/expo/node_modules:/mnt/e/Code/Nodejs/Zola/node_modules/.pnpm/expo@50.0.21_@babel+core@7._dc68a35d979f9b90e1209db8fa280efd/node_modules:/mnt/e/Code/Nodejs/Zola/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/Code/Nodejs/Zola/node_modules/.pnpm/expo@50.0.21_@babel+core@7._dc68a35d979f9b90e1209db8fa280efd/node_modules/expo/bin/node_modules:/mnt/e/Code/Nodejs/Zola/node_modules/.pnpm/expo@50.0.21_@babel+core@7._dc68a35d979f9b90e1209db8fa280efd/node_modules/expo/node_modules:/mnt/e/Code/Nodejs/Zola/node_modules/.pnpm/expo@50.0.21_@babel+core@7._dc68a35d979f9b90e1209db8fa280efd/node_modules:/mnt/e/Code/Nodejs/Zola/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../expo@50.0.21_@babel+core@7._dc68a35d979f9b90e1209db8fa280efd/node_modules/expo/bin/cli" "$@"
else
  exec node  "$basedir/../../../../../expo@50.0.21_@babel+core@7._dc68a35d979f9b90e1209db8fa280efd/node_modules/expo/bin/cli" "$@"
fi
